{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/iniciarSecion",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://nick-black.vercel.app"
      },
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
